<?xml version="1.0" encoding="UTF-8"?>
<org.wbsilva.bence.graphgrammar:TripleGrammar
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:org.wbsilva.bence.graphgrammar="org.wbsilva.bence.graphgrammar"
    xsi:schemaLocation="org.wbsilva.bence.graphgrammar ../../org.wbsilva.bence.graphgrammar/model/graphgrammar.ecore"
    name="a2b"
    terminals="//@alphabet.1 //@alphabet.2"
    nonterminals="//@alphabet.0"
    initial="//@alphabet.0">
  <alphabet
      name="S"/>
  <alphabet
      name="a"/>
  <alphabet
      name="b"/>
  <tripleRules>
    <source
        name="r0">
      <lhs name="S"/>
      <rhs>
        <vertices
            id="a">
          <label
              name="a"/>
        </vertices>
      </rhs>
    </source>
    <corr name="r0">
      <lhs name="S"/>
      <rhs>
        <vertices
            id="a">
          <label
              name="a"/>
        </vertices>
      </rhs>
    </corr>
    <target
        name="r0">
      <lhs name="S"/>
      <rhs>
        <vertices
            id="b">
          <label
              name="b"/>
        </vertices>
      </rhs>
    </target>
  </tripleRules>
</org.wbsilva.bence.graphgrammar:TripleGrammar>
